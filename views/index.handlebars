<h1 id="sectiontitle">SMACK THAT PUPPY</h1>
<img id="pomeranian" src="./assets/images/Pomeranian.png" />
<div class="container">
  <div class="row">
    <div class="col-md-5">
      <h2 id="nextsection"> Puppies Available</h2>

      <ul>
        {{#each dogs}}
        {{#unless smacked}}
        {{> cats/cat-block smacked=true}}
        {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="col-md-2">
      <h2 id="choosepuppy">Name A Puppy</h2>
      <form class="create-form">

        <div class="form-group">
          <label for="ca">Dog Breed:</label>
          <input type="text" id="ca" name="name">
        </div>

        <div class="form-group">
          <label for="smacked">Smack It</label><br>
          <input type="radio" name="smacked" value="1" checked> Smacked!<br>
          <input type="radio" name="smacked" value="0"> Un-smacked
        </div>

        <button type="submit">Add Puppy</button>
      </form>
    </div>
    <div class="col-md-5">
      <h2 id="nextsection"> Smacked To The Future!</h2>

      <ul>
        {{#each dogs}}
        {{#if smacked}}
        {{> cats/cat-block smacked=false}}
        {{/if}}
        {{/each}}
      </ul>
    </div>

  </div>
</div>